    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:50153",
      "sslPort": 44315
    }
  },
  "profiles": {
    "IIS Development": {
      "commandName": "IISExpress",
